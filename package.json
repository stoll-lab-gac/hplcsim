{
  "name": "hplcsim",
  "version": "5.0.0",
  "description": "Multi-Dimensional Separations' 1D-LC Simulator",
  "author": "Thomas J. Lauer",
  "license": "CC BY-NC-SA 4.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@stoll-lab-gac/chroma-core": "^1.1.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "d3-format": "^3.0.1",
    "plotly.js-cartesian-dist": "^2.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-plotly.js": "^2.5.1",
    "react-scripts": "5.0.0",
    "react-st-modal": "^1.1.3",
    "react-table": "^7.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "update": "npm i @stoll-lab-gac/chroma-core@latest",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js",
    "docs": "jsdoc2md src/*.js > docs.md",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!d3-format|react-plotly\\.js)/\" --env=jsdom",
    "deploy": "IF EXIST build RMDIR /S /Q build && npm install && npm run build && cd build && ren index.html index.php && cd .."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jsdoc": "~3.6.10",
    "jsdoc-to-markdown": "^7.1.1"
  }
}
